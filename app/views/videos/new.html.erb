<h1>Videos#new</h1>
<p>Find me in app/views/videos/new.html.erb</p>
<%= simple_form_for @video do |f| %>
  <%= f.input :name,
        label: "video name :" %>
  <%= f.input :section_id, collection: Section.all, label: "Section", label_method: :title %>
  <%= f.input :video,
              required: false,
              as: :file %>

  <%= f.button :submit, "Create Video" %>
<% end %>
